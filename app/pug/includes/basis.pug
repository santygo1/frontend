block page-config
	- var title = 'DEFAULT_TITLE'

doctype html
html(lang="ru")
	head
		meta(charset="UTF-8")
		meta(name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no")
		title #{title} 
		block links
			link(rel="stylesheet", href="css/main.css")
	body
		include _header
		block content
		include _footer
